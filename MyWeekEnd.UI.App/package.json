{
	"name": "myweekend.ui.app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:mobile": "npm run build && npx cap sync && npx cap open android",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"copysqlwasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm build/_app/immutable/assets"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"carbon-components-svelte": "^0.79.0",
		"carbon-icons-svelte": "^12.1.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.65.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"@capacitor-community/sqlite": "^5.0.7-2",
		"@capacitor/android": "^5.2.3",
		"@capacitor/cli": "^5.2.3",
		"@capacitor/core": "^5.2.3",
		"@capacitor/ios": "^5.2.3",
		"@capacitor/preferences": "^5.0.6",
		"@types/node": "^20.4.10",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.17",
		"uuid": "^9.0.0"
	}
}
